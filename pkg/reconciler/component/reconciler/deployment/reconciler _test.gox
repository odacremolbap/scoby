package deployment

import (
	"testing"

	"github.com/stretchr/testify/assert"
)

func TestReconciledObjectStatusEquality(t *testing.T) {
	testCases := map[string]struct {
		ro       ReconciledObject
		in       ReconciledObject
		expected bool
	}{}

	for name, tc := range testCases {
		t.Run(name, func(t *testing.T) {
			eq := tc.ro.StatusEqual(tc.in)

			assert.Equal(t, tc.expected, eq)
		})
	}
}
